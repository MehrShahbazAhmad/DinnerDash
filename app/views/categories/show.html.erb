<div class="container">
  <h1 class = 'pt-5'><%= @category.title %> Items</h1>
  <%= render 'table', category: @category %>
  <% if current_user.admin? %>
    <h1 class = 'pt-5'>Add New Items to this Category </h1>
    <%= render 'table2', items: @items,category: @category %>
  <% end %>

  <%= link_to 'Back', categories_path, class:"btn btn-dark"%>
</div>
